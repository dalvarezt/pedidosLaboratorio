                        .form-check.form-check-inline
                            input(
                                type="radio"
                                name="ignore.Electromiografia"
                                id="imagenOtrosServiciosElectromiografia4m"
                                value="Electromiografía 4 miembros"
                                onchange="if(this.checked){ $('#imagenOtrosServiciosElectromiografia').val(this.value)}"
                            ).form-check-input
                            label(for="imagenOtrosServiciosElectromiografia4m").form-check-label 4 miembros
            .row
                .col-md
                    .form-group
                        input(
                            type="hidden"
                            name="imagen.otrosServicios"
                            id="imagenOtrosServiciosOtros"
                        )
                        label(for="imagenOtrosServiciosHC") Datos de historia clínica:
                        textarea(
                            id="imagenOtrosServiciosHC"
                        ).form-control
                    .form-group
                        label(for="imagenOtrosServiciosDG") Diagnóstico presuntivo:
                        textarea(
                            id="imagenOtrosServiciosDG"
                        ).form-control
                    script. 
                        function imgOSChange(){
                            if(
                                $("#imagenOtrosServiciosHC").val() != "" ||
                                $("#imagenOtrosServiciosDG").val() != ""
                            ) {
                                $("#imagenOtrosServiciosOtros").val(
                                    "Datos de historia clínica: " + $("#imagenOtrosServiciosHC").val() +
                                    " - Diagnóstico presuntivo: " + $("#imagenOtrosServiciosDG").val() 
                                )
                            } else {
                                $("#imagenOtrosServiciosOtros").val("");
                            }
                        }
                        window.addEventListener("load", ()=> {
                            $("#imagenOtrosServiciosHC").bind("input cut paste", imgOSChange)
                            $("#imagenOtrosServiciosDG").bind("input cut paste", imgOSChange)
                        })
            .row.mb-4 &nbsp;

