div(class="card")
    div(class="card-header bg-secondary" id="imagenOtrosServiciosTitulo")
        h2(class="mb-0")
            button(
                class="btn btn-link text-white",
                type="button",
                data-toggle="collapse" 
                data-target="#imagenOtrosServicios",
                aria-expanded="true",
                aria-controls="imagenOtrosServicios"
            ) 
                span(data-toggle="collapse" data-target="#imagenOtrosServicios").arrow-toggle.collapsed
                    i.material-icons.icon-arrow-up expand_more
                    i.material-icons.icon-arrow-down expand_less
                | Otros Servicios
    div(id="imagenOtrosServicios" class="collapse hide" aria-labelledby="imagenOtrosServiciosTitulo" data-parent="#imagen")
        div(class="card-body")
            .row
                .col-sm
                    .checkbox
                        label
                            input(
                                type="checkbox" 
                                checked=false 
                                id="imagenOtrosServiciosElectrocardiograma" 
                                name="imagen.otrosServicios"
                                data-toggle="collapse"
                                data-target="#collapseElectrocardiograma"
                                value="Electrocardiograma"
                            ) 
                            label(
                                class="text-dark" 
                                for="imagenOtrosServiciosElectrocardiograma"
                            ) Electrocardiograma
                    .collapse.ml-4(id="collapseElectrocardiograma")
                        .form-check.form-check-inline
                            input(
                                type="radio"
                                name="ignore.electrocardiograma"
                                id="imagenOtrosServiciosElectrocardiogramaCI"
                                value="Electrocardiograma con interpretación"
                                onchange="if(this.checked){ $('#imagenOtrosServiciosElectrocardiograma').val(this.value)}"
                            ).form-check-input
                            label(for="imagenOtrosServiciosElectrocardiogramaCI").form-check-label con interpretación
                        .form-check.form-check-inline
                            input(
                                type="radio"
                                name="ignore.electrocardiograma"
                                id="imagenOtrosServiciosElectrocardiogramaSI"
                                value="Electrocardiograma sin interpretación"
                                onchange="if(this.checked){ $('#imagenOtrosServiciosElectrocardiograma').val(this.value)}"
                            ).form-check-input
                            label(for="imagenOtrosServiciosElectrocardiogramaSI").form-check-label sin interpretación
                .col-sm
                    .checkbox
                        label
                            input(
                                type="checkbox" 
                                checked=false 
                                id="imagenOtrosServiciosEspirometria" 
                                name="imagen.otrosServicios"
                                data-toggle="collapse"
                                data-target="#collapseEspirometria"
                                value="Espirometría"
                            ) 
                            label(
                                class="text-dark" 
                                for="imagenOtrosServiciosEspirometria"
                            ) Espirometría
                    .collapse.ml-4(id="collapseEspirometria")
                        .form-check.form-check-inline
                            input(
                                type="radio"
                                name="ignore.Espirometria"
                                id="imagenOtrosServiciosEspirometriaCI"
                                value="Espirometria con broncodilatador"
                                onchange="if(this.checked){ $('#imagenOtrosServiciosEspirometria').val(this.value)}"
                            ).form-check-input
                            label(for="imagenOtrosServiciosEspirometriaCI").form-check-label con broncodilatador
                        .form-check.form-check-inline
                            input(
                                type="radio"
                                name="ignore.Espirometria"
                                id="imagenOtrosServiciosEspirometriaSI"
                                value="Espirometria sin broncodilatador"
                                onchange="if(this.checked){ $('#imagenOtrosServiciosEspirometria').val(this.value)}"
                            ).form-check-input
                            label(for="imagenOtrosServiciosEspirometriaSI").form-check-label sin broncodilatador
